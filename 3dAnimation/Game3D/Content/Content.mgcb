
#----------------------------- Global Properties ----------------------------#

/outputDir:bin/$(Platform)
/intermediateDir:obj/$(Platform)
/platform:DesktopGL
/config:
/profile:Reach
/compress:False

#-------------------------------- References --------------------------------#

/reference:..\..\Newtonsoft.Json.dll
/reference:..\..\glTFLoader.dll
/reference:..\..\UmbrellaToolsKit.ContentPipeline.gltf\bin\Debug\net6.0\UmbrellaToolsKit.ContentPipeline.gltf.dll
